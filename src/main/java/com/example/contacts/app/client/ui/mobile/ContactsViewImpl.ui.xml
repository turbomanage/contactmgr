<ui:UiBinder
        xmlns:ui='urn:ui:com.google.gwt.uibinder'
        xmlns:g="urn:import:com.google.gwt.user.client.ui"
        xmlns:c="urn:import:com.google.gwt.user.cellview.client">

        <ui:style>
                table.listview {
                        width: 20em;
                        margin-left: auto;
                        margin-right: auto;
                }
                .listview div {
                        font-size: 2em;
                        line-height: 1.2;
                }
        </ui:style>
        <c:CellTable ui:field="contactCellTable" styleName="{style.listview}" />
</ui:UiBinder>